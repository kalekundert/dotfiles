export EDITOR=nvim
export BROWSER=qutebrowser
export PDF=zathura

export CC=$(which clang)
export CXX=$(which clang++)
export PYTHONSTARTUP=~/.pythonrc
export GPG_TTY=$(tty)
export ROSETTA=~/rosetta/master
export ROSETTA_BUILD=linuxclangrelease
export NINJA_STATUS="[%f/%t] [-j%r] "
export NVIM_GTK_NO_HEADERBAR=1
export TEXMFHOME="$HOME/.config/texlive"

export YSU_IGNORED_ALIASES=("l" "s" "cd..")
export AGKOZAK_USER_HOST_DISPLAY=0
export AGKOZAK_BLANK_LINES=1
export AGKOZAK_PROMPT_CHAR=( '$' '#' ':' )
export AGKOZAK_PROMPT_CHAR=( ❯ '%F{red}❯%f' ❮ )
export AGKOZAK_CUSTOM_SYMBOLS=( '⇣⇡' '⇣' '⇡' '+' 'x' '!' '»' '?' )

